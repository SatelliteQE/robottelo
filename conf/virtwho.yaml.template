        GUEST_PASSWORD:
    LIBVIRT:
        HYPERVISOR_TYPE: libvirt
        HYPERVISOR_SERVER:
        HYPERVISOR_USERNAME:
        HYPERVISOR_PASSWORD:
        GUEST:
        GUEST_PORT:
