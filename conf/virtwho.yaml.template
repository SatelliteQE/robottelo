        GUEST_PASSWORD:
    HYPERV:
        HYPERVISOR_TYPE: hyperv
        HYPERVISOR_SERVER:
        HYPERVISOR_USERNAME:
        HYPERVISOR_PASSWORD:
        GUEST:
        GUEST_PORT:
        GUEST_USERNAME:
        GUEST_PASSWORD:
    RHEVM:
        HYPERVISOR_TYPE: rhevm
        HYPERVISOR_SERVER:
        HYPERVISOR_USERNAME:
        HYPERVISOR_PASSWORD:
        GUEST:
        GUEST_PORT:
