      # BASEOS: replace-with-rhel8-os-baseos-http-link
      # APPSTREAM: replace-with-rhel8-os-appstream-http-link
  # RHEL7_OPTIONAL: replace-with-rhel7-optional-url
  # RHEL7_EXTRAS: replace-with-rhel7-extras-url
